---
import type { IconProps } from '../../lib/icons';

const { class: className, size = 20 } = Astro.props as IconProps;
---

<svg
	xmlns="http://www.w3.org/2000/svg"
	width={size}
	height={size}
	viewBox="0 0 24 24"
	fill="none"
	stroke="currentColor"
	stroke-width="2"
	stroke-linecap="round"
	stroke-linejoin="round"
	class={className}
>
	<polyline points="6 9 12 15 18 9"></polyline>
</svg>
